            "name": "Python Debugger: lobster-doxygen with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/lobster-doxygen/__main__.py",
            "console": "integratedTerminal",
            "args": [
                "-v",
                "-o",
                ".\\examples\\simple-cpp-application\\out\\simple-cpp-application-lobster.json",
                ".\\examples\\simple-cpp-application\\out\\xml",
